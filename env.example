# AI Provider Configuration
OPENAI_API_KEY=your-openai-api-key

# Database Configuration
MONGODB_CONNECTION_STRING=mongodb://localhost:27017/rowboat
DATABASE_URL=your-database-url

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Authentication (Auth0)
USE_AUTH=false
AUTH0_SECRET=your-auth0-secret-here
AUTH0_BASE_URL=http://localhost:3001
AUTH0_ISSUER_BASE_URL=https://your-domain.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret

# API Keys for Internal Services
AGENTS_API_KEY=your-agents-api-key
COPILOT_API_KEY=your-copilot-api-key

# RAG (Retrieval Augmented Generation) Configuration
USE_RAG=true
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your-qdrant-api-key

# File Upload Configuration
USE_RAG_UPLOADS=true
USE_RAG_S3_UPLOADS=false
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
RAG_UPLOADS_S3_BUCKET=your-s3-bucket
RAG_UPLOADS_S3_REGION=us-east-1

# Web Scraping
USE_RAG_SCRAPING=true
FIRECRAWL_API_KEY=your-firecrawl-api-key

# Chat Widget
USE_CHAT_WIDGET=false
CHAT_WIDGET_HOST=http://localhost:3006
CHAT_WIDGET_SESSION_JWT_SECRET=your-jwt-secret

# Rate Limiting
MAX_QUERIES_PER_MINUTE=60
MAX_PROJECTS_PER_USER=10

# Voice API
VOICE_API_URL=your-voice-api-url

# AI Model Configuration
PROVIDER_BASE_URL=https://api.openai.com/v1
PROVIDER_API_KEY=your-provider-api-key
PROVIDER_DEFAULT_MODEL=gpt-4
PROVIDER_COPILOT_MODEL=gpt-3.5-turbo

# Embedding Configuration
EMBEDDING_PROVIDER_BASE_URL=https://api.openai.com/v1
EMBEDDING_PROVIDER_API_KEY=your-embedding-api-key
EMBEDDING_MODEL=text-embedding-ada-002
EMBEDDING_VECTOR_SIZE=1536

# Agent Configuration
MAX_CALLS_PER_CHILD_AGENT=10
ENABLE_TRACING=false

# Google Services
GOOGLE_API_KEY=your-google-api-key
USE_GEMINI_FILE_PARSING=false

# External Integrations
KLAVIS_API_KEY=your-klavis-api-key
KLAVIS_GITHUB_CLIENT_ID=your-github-client-id
KLAVIS_GOOGLE_CLIENT_ID=your-google-client-id

# Development/Debug
NODE_ENV=development 